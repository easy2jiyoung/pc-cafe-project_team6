<!DOCTYPE html>
<html lang="ko">

<head>
    <%- include('head.ejs') %>

        <title></title>
</head>

<body>
    <div class="col-1">
        <button type="button" class="btn btn-danger" onclick="logOut()">로그아웃</button>
    </div>

    <script>
        function logOut() {
            $.ajax({
                type: 'GET',
                url: '/api/users/logout',
                success: function (response) {
                    sessionStorage.clear()
                    window.location.href = '/'
                },
                error: function (response) {
                    alert(response)
                }
            })
        }
    </script>
</body>